[push]
	default = simple
    followTags = true
[user]
	name = <FillThisone>
	email = <FillThisOne>
[alias]
    st = status
    ci = commit
    br = branch
    co = checkout
    df = diff
    dc = diff --cached
    lg = log -p
    lol = log --graph --decorate --pretty=oneline --abbrev-commit
    lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
    ls = ls-files
[core]
    autocrlf = input
